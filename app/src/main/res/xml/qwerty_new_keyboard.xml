        <Key
            android:codes="46"
            android:isRepeatable="true"
            android:keyWidth="10%p"
            android:keyLabel="." />

        <Key
            android:codes="-4"
            android:horizontalGap="1.8%p"
            android:keyWidth="18%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_enter_green" />
    </Row>
